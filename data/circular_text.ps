% Ouotside circle
/outsidecircletext {
    circtextdict begin
        /radius exch def
        /centerangle exch def
        /ptsize exch def
        /str exch def
        /xradius radius ptsize 4 div add def
        gsave
            centerangle str findhalfangle add rotate
            str {
                /charcode exch def
                ( ) dup 0 charcode put outsideplacechar
            } forall
        grestore
    end
} def

% Inside circle
/insidecircletext {
    circtextdict begin
        /radius exch def
        /centerangle exch def
        /ptsize exch def
        /str exch def
        /xradius radius ptsize 3 div sub def
        gsave
            centerangle str findhalfangle sub rotate
            str {
                /charcode exch def
                ( ) dup 0 charcode put insideplacechar
            } forall
        grestore
    end
} def

%
/circtextdict 16 dict def

%
circtextdict begin
    /findhalfangle {
        stringwidth pop 2 div
        2 xradius mul pi mul div 360 mul
    } def
    /outsideplacechar {
        /char exch def
        /halfangle char findhalfangle def
        gsave
            halfangle neg rotate
            radius 0 translate
            -90 rotate
            char stringwidth pop 2 div neg 0 moveto
            char show
        grestore
        halfangle 2 mul neg rotate
    } def
    /insideplacechar {
        /char exch def
        /halfangle char findhalfangle def
        gsave
            halfangle rotate
            radius 0 translate
            90 rotate
            char stringwidth pop 2 div neg 0 moveto
            char show
        grestore
        halfangle 2 mul rotate
    } def
    /pi 3.1415923 def
end

%
/doACircle {
    /radius exch def
    0 0 radius 0 360 arc closepath fill
    0 setgray
    0 0 radius 0 360 arc stroke
} def

% Mainline
306 448 translate

gsave
10 setlinewidth
0.6 0.9 0.2 setrgbcolor
170 doACircle
grestore

gsave
5 setlinewidth
1 setgray
130 doACircle
grestore

/Times-Bold findfont 22 scalefont setfont
(Symphony No. 9 (The Choral Symphony)) 22 90 142 outsidecircletext

/Times-Roman findfont 15 scalefont setfont
(Ludwig von Beethoven) 15 90 114 outsidecircletext

(The New York Philharmonic Orchestra) 15 270 120 insidecircletext

gsave
/Times-Bold findfont 250 scalefont setfont
-65 -73 moveto
(1) show
grestore

showpage

